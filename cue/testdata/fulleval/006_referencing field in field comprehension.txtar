#name: referencing field in field comprehension
#evalFull
-- in.cue --
a: {b: c: 4}
a: {
	b: d: 5
	for k, v in b {
		"\(k)": v
	}
}
-- out/def --
a: {
	b: {
		c: 4
		d: 5
	}
	c: 4
	d: 5
}
-- out/export --
a: {
	b: {
		c: 4
		d: 5
	}
	c: 4
	d: 5
}
-- out/yaml --
a:
  b:
    c: 4
    d: 5
  c: 4
  d: 5
-- out/json --
{"a":{"b":{"c":4,"d":5},"c":4,"d":5}}
-- out/legacy-debug --
<0>{a: <1>{b: <2>{c: 4, d: 5}, c: 4, d: 5}}
