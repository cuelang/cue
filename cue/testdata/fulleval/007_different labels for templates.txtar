#name: different labels for templates
#evalFull
-- in.cue --
a: [X=string]: {name: X}
a: [Name=string]: {name: Name}
a: foo: {}
-- out/def --
a: {
	{[X=string]: {
		name: X & X
	}}
	foo: {
		name: "foo"
	}
}
-- out/export --
a: {
	foo: {
		name: "foo"
	}
}
-- out/yaml --
a:
  foo:
    name: foo
-- out/json --
{"a":{"foo":{"name":"foo"}}}
-- out/legacy-debug --
<0>{a: <1>{[]: <2>(X: string)->(<3>{name: <2>.X} & <4>{name: <2>.X}), foo: <5>{name: "foo"}}}
