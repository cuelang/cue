#name: field comprehension
#evalFull
-- in.cue --
a: {
	for k, v in b
	if k < "d"
	if v > b.a {
		"\(k)": v
	}
}
b: {
	a: 1
	b: 2
	c: 3
	d: 4
}
c: {
	for k, v in b
	if k < "d"
	if v > b.a {
		"\(k)": v
	}
}
-- out/def --
a: {
	b: 2
	c: 3
}
b: {
	a: 1
	b: 2
	c: 3
	d: 4
}
c: {
	b: 2
	c: 3
}
-- out/export --
a: {
	b: 2
	c: 3
}
b: {
	a: 1
	b: 2
	c: 3
	d: 4
}
c: {
	b: 2
	c: 3
}
-- out/yaml --
a:
  b: 2
  c: 3
b:
  a: 1
  b: 2
  c: 3
  d: 4
c:
  b: 2
  c: 3
-- out/json --
{"a":{"b":2,"c":3},"b":{"a":1,"b":2,"c":3,"d":4},"c":{"b":2,"c":3}}
