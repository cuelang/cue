#name: json Marshaling detects incomplete
#evalFull
-- in.cue --
import "encoding/json"

a: json.Marshal({a: string})

foo: {a: 3, b: foo.c}
b: json.Marshal(foo)
-- out/def --
import "encoding/json"

a: json.Marshal({
	a: string
})
foo: {
	a: 3
	b: foo.c
}
b: json.Marshal(foo)
-- out/legacy-debug --
<0>{a: <1>.Marshal (<2>{a: string}), foo: <3>{a: 3, b: <4>.foo.c}, b: <1>.Marshal (<4>.foo)}
