#name: associativity of defaults
#evalFull
-- in.cue --
a: *"a" | ("b" | "c")
b: (*"a" | "b") | "c"
c: *"a" | (*"b" | "c")
x: a & b
y: b & c
-- out/def --
x: a & b
y: b & c
a: *"a" | "b" | "c"
b: *"a" | "b" | "c"
c: *"a" | *"b" | "c"
-- out/legacy-debug --
<0>{x: "a", y: (*"a" | *"b"), a: "a", b: "a", c: (*"a" | *"b")}
