#name: self-reference cycles conflicts with strings
#evalPartial
-- in.cue --
a: {
	x: y + "?"
	y: x + "!"
}
a: x: "hey"
-- out/def --
a: {
	x: _|_ // conflicting values "hey!?" and "hey"
	y: "hey!"
}
-- out/legacy-debug --
<0>{a: <1>{x: _|_(("hey!?" & "hey"):conflicting values "hey!?" and "hey"), y: "hey!"}}
